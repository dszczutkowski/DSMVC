@model IEnumerable<DSMVC.Models.Meal>

@{
    ViewBag.Title = "Nazwy";
}

<h2>Nasze dania</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.ActionLink("Więcej", "Index")
            </td>
        </tr>
    }

</table>

